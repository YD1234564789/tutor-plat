<div class="row">
  <div class="col-md-12">
    <h1>{{user.name}}</h1>
  </div>
  <div class="col-md-4">
    <div class="well">
      <ul class="list-unstyled">
        <li> {{user.avatar}}</li>
        <li><strong>Country：</strong>{{user.country}}</li>
        <li><strong>Email：</strong> {{user.email}}</li>
        <li><strong>TotalLearningTime：</strong>{{user.TotalLearningTime}}</li>
      </ul>
    </div>
  </div>
  <div class="col-md-8">
    <p style="white-space: break-spaces;">{{user.description}}</p>
  </div>
  <hr>
</div>
<a class="btn btn-outline-secondary" href="javascript:history.back()">Back</a>